{
    "manifest_version": 3,
    "name": "Youtube Auto Skip Ads",
    "description": "Skip ads while playing videos on YouTube",
    "version": "2.0.0",
    "icons": {
        "16": "icons/16x16.png",
        "32": "icons/32x32.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "https://www.youtube.com/*"
            ]
        }
    ]
}
